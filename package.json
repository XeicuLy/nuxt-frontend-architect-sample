{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "eslint:ci": "eslint . --max-warnings 0",
    "typecheck": "nuxi typecheck .",
    "lint": "pnpm eslint && pnpm biome && pnpm prettier && pnpm typecheck",
    "lint:fix": "pnpm eslint:fix && pnpm biome:fix && pnpm prettier:fix && pnpm typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest run --coverage",
    "generate-types": "openapi-ts && pnpm biome:fix"
  },
  "dependencies": {
    "@hono/swagger-ui": "0.5.2",
    "@hono/zod-openapi": "1.1.0",
    "@hono/zod-validator": "0.7.2",
    "@tanstack/vue-query": "5.85.5",
    "consola": "3.4.2",
    "hono": "4.9.4",
    "zod": "4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@hey-api/openapi-ts": "0.81.0",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@pinia/nuxt": "0.11.2",
    "@pinia/testing": "1.0.2",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.33.0",
    "eslint-plugin-tailwindcss": "3.18.2",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "globals": "16.3.0",
    "happy-dom": "17.6.3",
    "nuxt": "4.0.3",
    "pinia": "3.0.3",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "vue": "3.5.18",
    "vue-router": "4.5.1",
    "vue-tsc": "3.0.6"
  },
  "volta": {
    "node": "24.7.0",
    "pnpm": "10.15.0"
  }
}
